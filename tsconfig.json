{
    "version": "1.6.2",
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outDir": "./lib",
        "rootDir": "./src",
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./src/**/*.ts",
        "./typings/**/*.ts"
    ],
    "files": [
        "./src/client.ts",
        "./src/defer.ts",
        "./src/extensions/presence-batch.ts",
        "./src/extensions/resume.ts",
        "./src/interfaces.d.ts",
        "./src/main.ts",
        "./typings/es6-promise/es6-promise.d.ts",
        "./typings/jasmine/jasmine.d.ts",
        "./typings/node/node.d.ts",
        "./typings/shortid/shortid.d.ts",
        "./typings/tsd.d.ts",
        "./typings/vendor/debug.d.ts",
        "./typings/vendor/object-tree.d.ts",
        "./typings/ws/ws.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
